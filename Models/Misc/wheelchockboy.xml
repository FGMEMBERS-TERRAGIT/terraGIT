<?xml version="1.0" encoding="UTF-8" ?>
<PropertyList>
	<!-- 09.04.2013-Andre Burkert	--> 
	<path>wheelchockboy.ac</path>
<animation>
	<type>alpha-test</type>
	<alpha-factor>0.01</alpha-factor>
</animation>
<animation>
	<name>wheelchockboy</name>
	<object-name>wheelchockboyinternal</object-name>
	<object-name>wheelchockboyexternal</object-name>
</animation>
 <animation>
	<type>billboard</type>
	<object-name>wheelchockboy</object-name>
	<spherical type="bool">false</spherical>
</animation>
<animation>
	<enable-hot type="bool">false</enable-hot>
</animation>
<animation>
	<type>textranslate</type> 
	<object-name>wheelchockboy</object-name>
	<property>/sim/time/sun-angle-rad</property>
	<step> 1.57 </step>
	<factor>0.318471338</factor>
	<axis> 
		<x> -1 </x> 
		<y>  0 </y> 
	</axis> 
</animation>
<animation>
	<type>material</type>
	<object-name>wheelchockboy</object-name>
		<condition>		
		<greater-than>		
		<property>/sim/time/sun-angle-rad</property>
		<value>1.57</value>
		</greater-than>
		</condition>
		<emission>	
			<red>0.6</red>	
			<green>0.6</green>	
			<blue>0.6</blue>
		</emission>
</animation>
<animation>
	<type>range</type>
	<object-name>wheelchockboyinternal</object-name>
	<min-m>0</min-m>
	<max-m>33</max-m>
</animation>
<animation>
	<type>range</type>
	<object-name>wheelchockboyexternal</object-name>
	<min-m>0</min-m>
	<max-m>65</max-m>
</animation>
<animation>
  <type>select</type>
	<object-name>wheelchockboyexternal</object-name>
  <condition>
   <not>
    <property>sim/current-view/internal</property>
   </not>
  </condition>
 </animation>
</PropertyList>
