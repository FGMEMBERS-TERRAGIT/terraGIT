    </axis>
  </animation>
  
      <animation>
    <name>rightprop</name>
    <object-name>Cube.001</object-name>
    <object-name>Circle.004</object-name>
    </animation>
  
      <animation>
    <type>select</type>
    <object-name>Circle.004</object-name>
    <condition>
      <greater-than>
        <property>velocities/true-airspeed-kt</property>
        <value>50</value>
      </greater-than>
    </condition>
  </animation>
    
      <animation>
    <type>select</type>
    <object-name>Cube.001</object-name>
