<?xml version="1.0" encoding="UTF-8" ?>

<PropertyList>

<path>Bruna_Alpina_cow.ac</path>

	<!-- Summer / Winter selection -->
	<animation>
		<type>select</type>
		<object-name>cow</object-name>
			<condition>
				<equals>
				<property>sim/startup/season</property>
				<value>summer</value>
				</equals>
			</condition>
	</animation>
	
	<!-- LOD -->
	<animation>
		<type>range</type>
		<min-m>0</min-m>
		<max-m>2000</max-m>
	</animation>
	
</PropertyList>
